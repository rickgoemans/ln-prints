{
    "name": "rgoemans/ln-prints-application",
    "type": "project",
    "description": "LN Prints application.",
    "keywords": [
        "LN",
        "Prints",
        "Website",
        "Application"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "artesaos/seotools": "^0.19.1",
        "bayareawebpro/nova-field-ckeditor": "^1.0",
        "biscolab/laravel-recaptcha": "^5.0",
        "coderello/laravel-nova-lang": "^1.6",
        "ebess/advanced-nova-media-library": "^3.3",
        "eminiarts/nova-tabs": "^1.2",
        "epartment/nova-dependency-container": "^1.2",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "kabbouchi/nova-impersonate": "^1.3",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.1",
        "laravel/nova": "~3.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lnprints/black-red-theme": "*",
        "maatwebsite/laravel-nova-excel": "^1.2",
        "sbine/route-viewer": "^0.0.7",
        "sidis405/nova-installed-packages": "^0.1.8",
        "spatie/laravel-activitylog": "^4.0",
        "spatie/laravel-honeypot": "^4.0",
        "spatie/nova-backup-tool": "^4.0",
        "titasgailius/search-relations": "^1.0",
        "vyuldashev/nova-permission": "^2.9"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.8",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "larastan/larastan": "^1.0",
        "laravel/sail": "^1.4",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse --memory-limit=2G",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan horizon:install",
            "@php artisan nova:install",
            "@php artisan telescope:install"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan horizon:publish",
            "@php artisan nova:publish",
            "@php artisan telescope:publish"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "@php artisan horizon:publish",
            "@php artisan nova:publish",
            "@php artisan telescope:publish"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/BlackRedTheme"
        }
    ]
}
